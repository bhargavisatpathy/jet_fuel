<!DOCTYPE html>
<html>
<head>
  <title>JetFuel</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <h1>JET FUEL</h1>
  <% flash.each do |name, message| %>
    <div class="<%= name %>">
      <p><%= message %></p>
    </div>
  <% end %>
  <%= yield %>

</body>
</html>
